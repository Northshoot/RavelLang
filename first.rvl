streaming model Random:
    properties:
        records=50
        test=name

    schema:
        time=TimestampField(auto_add=True)


controller RandomController:
    configuration:
        test=5



    event test.here():
        test=5

space EmbeddedSpace:
    properties:
        language=clang

    configuration:
        platform_templates=nrf52

    models:
        rndmodel=Random(size=10)
        frk=Datavalue(size=1)


    controllers:
        rndcnt = RandomController(tier=this)
