streaming model Random:

    properties:
        records=50

    schema:
        time=TimestampField
        number=Integer



controller RandomController:
    configuration:
        test = 5

    event test.here():
        test = 5

space EmbeddedSpace:
    properties:
        language=clang

    configuration:
        platform_templates = nrf52

    models:
        rndmodel=Random
        frk=Datavalue


    controllers:
        rndcnt = RandomEmbCntr
