class(package,className,inherits="Object",fields=[],methods=[]) ::= <<
package <package>;

public class <className> extends <inherits> {
    <fields:{f |<f>;
    }>

    <constructor(className)>

    <methods:{m |<m>;
    }>
};
>>

// default constructor is public and empty
constructor(className) ::= <<
public <className>() {}
>>