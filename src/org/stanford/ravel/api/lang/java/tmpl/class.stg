do_import(imp) ::=<<
import <imp>;
>>

class(package,imports,className,inherits="Object",members=[]) ::= <<
package <package>;

<imports:do_import()>

public class <className> extends <inherits> {
    <constructor(className)>

    <members:{m |<m>
    }>
};
>>

// default constructor is public and empty
constructor(className) ::= <<
public <className>() {}
>>